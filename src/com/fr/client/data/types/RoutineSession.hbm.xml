<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.fr.client.data.types.RoutineSession" table="RoutineSession">
        <id name="routineSessionID" type="long">
            <generator class="native"/>
        </id>
        <property name="userRoutineID" type="long" />
        <property name="routineSessionName" type="string" />
        <property name="orderIndex" type="int" />
    </class>

</hibernate-mapping>